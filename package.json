{
    "name": "@links/monorepo",
    "private": true,
    "version": "0.0.1",
    "workspaces": {
        "packages": [
            "packages/*",
            "services/*"
        ],
        "installConfig": {
            "hoistingLimits": "dependencies"
        }
    },
    "scripts": {
        "api:start:dev": "yarn workspace @links/api start:dev",
        "web:start:dev": "yarn workspace @links/web start:dev"
    },
    "packageManager": "yarn@3.2.0"
}
